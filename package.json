{
  "name": "mira-template",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack --progress -c config/webpack/prod.js",
    "watch": "webpack --watch  --progress -c config/webpack/prod.js",
    "serve": "webpack serve  --progress -c config/webpack/dev.js",
    "analyze": "webpack --analyze -c config/webpack/analyze.js",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint --ext ts --ext tsx"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Delave-las-Kure/project_teamplate_D"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "compilerOptions": {
    "typeRoots": [
      "./node_modules/@types",
      "./typings"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 4 chrome version",
      "last 4 firefox version",
      "last 4 safari version"
    ]
  },
  "licenses": [],
  "dependencies": {
    "hamburgers": "^1.1.3",
    "mmenu-js": "^8.5.22"
  },
