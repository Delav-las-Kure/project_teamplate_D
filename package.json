    "prettier-eslint-cli": "^5.0.1",
    "pretty-quick": "^3.1.0",
    "pug": "^3.0.2",
    "pug-loader": "^2.4.0",
    "sass": "^1.32.10",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4",
    "webpack": "^5.33.2",
    "webpack-cli": "^4.6.0",
